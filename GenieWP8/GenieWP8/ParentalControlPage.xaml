                                  Padding="10,0,0,0" Foreground="White" Style="{StaticResource CheckBoxStyle1}"/>
                        <Line Width="400" Stroke="White" X2="400"/>
                    </StackPanel>
                </ListBox>
                <Button Content="Cancel" Width="300" Click="CategoriesCancelBtn_Click" BorderBrush="White" Foreground="White"/>
            </StackPanel>
        </Popup>

        <Popup x:Name="SettingCompletePopup" Grid.Row="1"  HorizontalAlignment="Center" Margin="0,0,400,0">
            <StackPanel>
                <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Margin="0,10,0,0">
                    <TextBlock Text="{Binding Path=LocalizedResources.SettingCompleteText, Source={StaticResource LocalizedStrings}}"
                                   FontSize="30" FontWeight="Bold" VerticalAlignment="Center" Foreground="White"/>
                </StackPanel>
                <ScrollViewer x:Name="SettingCompleteScrollViewer" Width="400" Height="400" VerticalScrollBarVisibility="Auto" Foreground="White">
                    <StackPanel>
                        <TextBlock Text="{Binding Path=LocalizedResources.CompleteIllustrationText, Source={StaticResource LocalizedStrings}}"
                               FontSize="24" Margin="0,10,0,0" TextWrapping="Wrap"/>
                        <TextBlock Text="{Binding Path=LocalizedResources.Illustration1Text, Source={StaticResource LocalizedStrings}}"
                               FontSize="24" Margin="0,20,0,0" TextWrapping="Wrap"/>
                        <TextBlock Text="{Binding Path=LocalizedResources.Illustration2Text, Source={StaticResource LocalizedStrings}}"
                               FontSize="24" Margin="0,20,0,0" TextWrapping="Wrap"/>
                        <TextBlock Text="{Binding Path=LocalizedResources.Illustration3Text, Source={StaticResource LocalizedStrings}}"
                               FontSize="24" Margin="0,20,0,0" TextWrapping="Wrap"/>
                        <TextBlock Text="{Binding Path=LocalizedResources.Illustration4Text, Source={StaticResource LocalizedStrings}}"
                               FontSize="24" Margin="0,20,0,0" TextWrapping="Wrap"/>
                    </StackPanel>
                </ScrollViewer>
                <Button x:Name="ReturnToStatusButton" Content="{Binding Path=LocalizedResources.ReturnButtonContent, Source={StaticResource LocalizedStrings}}"
                            Click="ReturnToStatusButton_Click" FontSize="22" Width="250" HorizontalAlignment="Center" VerticalAlignment="Top" BorderBrush="White" Foreground="White"/>
            </StackPanel>
        </Popup>

        <Popup x:Name="PopupEnquireBypassAccount" Grid.Row="1"  HorizontalAlignment="Center" Margin="0,20,400,0">
            <StackPanel>
                <TextBlock Text="Please login your Live Parental Controls account and add a bypass account."
                           FontSize="30" Width="400" VerticalAlignment="Center" Margin="10,0,0,0" TextWrapping="Wrap" Foreground="White"/>
                <Grid>
                    <Button x:Name="CancelButton" Content="{Binding Path=LocalizedResources.CancelButtonContent, Source={StaticResource LocalizedStrings}}"
                            Click="CancelButton_Click" Grid.Row="1" FontSize="22" Width="200" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,200,0"
                            BorderBrush="White" Foreground="White"/>
                    <Button x:Name="OkButton" Content="{Binding Path=LocalizedResources.OkButtonContent, Source={StaticResource LocalizedStrings}}"
                            Click="OkButton_Click" Grid.Row="1" FontSize="22" Width="200" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="200,0,0,0"
                            BorderBrush="White" Foreground="White"/>
                </Grid>
            </StackPanel>
        </Popup>
    </Grid>

</phone:PhoneApplicationPage>